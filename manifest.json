{
  "name": "YouTube Search Bar",
  "version": "1.1.1",
  "description": "Avoid YouTube home screen",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+1"
      },
      "description": "Open the extension popup"
    }
  },
  "manifest_version": 3
}